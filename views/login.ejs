      #header {
        min-height: 100vh;
        width: 100%;
        background-image: linear-gradient(
            rgba(4, 9, 30, 0.7),
            rgba(4, 9, 30, 0.7)
          ),
          url(https://www.savethechildren.in/wp-content/uploads/2021/10/Artboard-2-1.jpg);
        background-position: center;
        background-size: cover;
        position: relative;
        scroll-behavior: smooth;
      }
      nav {
        display: flex;
        padding: 2% 6%;
        justify-content: space-between;
        align-items: center;
      }

      nav img {
        position: relative;
    top: -22px;
    left: -76px;
    width: 65px;
    width: 11%;
      }
      .nav-links {
        flex: 1;
        text-align: right;
        position: relative;
        top: -36px;
        right: 40px;
      }

      .nav-links ul li {
        list-style: none;
        display: inline-block;
        padding: 8px 12px;
        position: relative;
      }
      .nav-links ul li a {
        color: white;
        text-decoration: none;
        font-size: 13px;
      }
      /* .header a{
            position: relative;
            height: 20px;
            width: 25%;
        } */

      .nav-links ul li::after {
        content: "";
        width: 0;
        height: 2px;
        background: #f44336;
        display: block;
        margin: auto;
        transition: 1s;
      }
      .nav-links ul li:hover:after {
        width: 100%;
        transition: 0.5s;
      }

      .log {
        position: relative;
        float: right;
        top: -670px;
        right: 23px;
        width: 91px;
        height: 31px;
        border-radius: 20px;
      }

      .log:hover {
        border: 1px solid #f44336;
        background: #f44336;
        transition: 0.5s;
      }
      .text-box {
        width: 90%;
        color: #fff;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        text-align: center;
      }
      .text-box h1 {
        font-size: 62px;
      }

      .text-box p {
        margin: -14px 0px 40px;
        font-size: 16px;
        color: white;
      }

